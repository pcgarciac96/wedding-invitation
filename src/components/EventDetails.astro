---
interface Props {
  title?: string; // "Boda"
  dateLine1: string; // "Sábado 14 de Marzo de 2026"
  dateLine2?: string; // "4:00 pm"
  venueTitle?: string; // "Lugar"
  venueName: string; // "Amazilia Hotel"
  venueAddress?: string; // "Vía Pereira a Cerritos..."
  rsvpHref?: string; // Link a WhatsApp/Google Form/Email
  mapsHref?: string; // Link a Google Maps
  class?: string;
}

const {
  title = "Boda",
  dateLine1,
  dateLine2 = "",
  venueTitle = "Lugar",
  venueName,
  venueAddress = "",
  rsvpHref,
  mapsHref,
  class: extra = "",
} = Astro.props;
---

<section
  class={`mx-auto w-full max-w-[820px] px-4 py-8 ${extra}`}
  style="--cream:#FBF4E8; --sepia:#8B6B5E; --accent:#D8C5A8;"
>
  <div
    class="rounded-[28px] md:rounded-[32px] md:border md:border-[color:var(--accent)]/60 bg-[#FDF4D4] p-6 md:p-10 text-center text-[color:var(--sepia)] md:shadow-md"
  >
    <!-- Ícono de anillos -->
    <div class="flex justify-center mb-4 md:mb-6 w-full text-[color:var(--sepia)]">
      <svg
        class=""
        width="126"
        height="126"
        viewBox="0 0 126 126"
        fill="none"
        xmlns="http://www.w3.org/2000/svg"
        xmlns:xlink="http://www.w3.org/1999/xlink"
      >
        <rect width="126" height="126" fill="url(#pattern0_21_45)"></rect>
        <defs>
          <pattern
            id="pattern0_21_45"
            patternContentUnits="objectBoundingBox"
            width="1"
            height="1"
          >
            <use xlink:href="#image0_21_45" transform="scale(0.00195312)"></use>
          </pattern>
          <image
            id="image0_21_45"
            width="512"
            height="512"
            preserveAspectRatio="none"
            xlink:href="data:image/png;base64,"
          />
        </defs>
      </svg>
    </div>

    <!-- Título -->
     <div class="flex justify-center ">
        <div class="flex justify-center border-y border-[#B88358] w-48">
          <h2
            id="guests-title"
            class="relative inline-block font-italianno text-6xl md:text-4xl text-[#B88358]"
          >
            <span
              class="before:absolute before:-top-3 before:left-1/2 before:-translate-x-1/2 before:block before:h-px before:w-40 before:bg-[color:var(--accent)/.7]
                 after:absolute after:-bottom-3 after:left-1/2 after:-translate-x-1/2 after:block after:h-px after:w-40 after:bg-[color:var(--accent)/.7]"
            >
              {title}
            </span>
          </h2>
        </div>
      </div>
    <!-- <div class="mx-auto my-2 h-px w-40 md:w-56 bg-[color:var(--sepia)]/50">
    </div>

    <h3 class="font-italianno text-4xl md:text-5xl mb-2">{title}</h3>
    <div class="mx-auto my-2 h-px w-40 md:w-56 bg-[color:var(--sepia)]/50">
    </div> -->

    <!-- Layout: móvil en columna / escritorio en dos columnas -->
    <div class="mt-6 grid gap-8 md:grid-cols-2 md:gap-10">
      <!-- Columna: Día -->
      <div>
        <h4 class="font-italianno text-5xl md:text-4xl mb-3 text-[#B88358]">Día</h4>
        <p class="font-italianno  text-3xl md:text-lg opacity-90 text-[#B88358]">{dateLine1}</p>
        {
          dateLine2 && (
            <p class="font-italianno  text-3xl md:text-lg opacity-90 text-[#B88358]">{dateLine2}</p>
          )
        }

        {
          rsvpHref && (
            <a
              href={rsvpHref}
              target="_blank"
              rel="noopener"
              class="h-10 py-2 w-48 mt-5 inline-block font-medium rounded-2xl bg-[#E6DDC0] text-[#B89477]  hover:bg-[color:var(--sepia)]/15 transition"
              aria-label="Confirmar asistencia"
            >
              Confirmar Asistencia
            </a>
          )
        }
      </div>

      <!-- Columna: Lugar -->
      <div>
        <h4 class="font-italianno text-5xl md:text-4xl mb-3 text-[#B88358]">{venueTitle}</h4>
        <p class="font-italianno  text-3xl md:text-lg opacity-90 text-[#B88358]">{venueName}</p>
        {venueAddress && <p class="font-italianno  text-2xl md:text-lg opacity-90 text-[#B88358]">{venueAddress}</p>}

        {
          mapsHref && (
            <a
              href={mapsHref}
              target="_blank"
              rel="noopener"
              class="h-10 py-2 w-48 mt-5 inline-block font-medium rounded-2xl bg-[#E6DDC0] text-[#B89477]  hover:bg-[color:var(--sepia)]/15 transition"
              aria-label="Cómo llegar"
            >
              Cómo llegar
            </a>
          )
        }
      </div>
    </div>
  </div>
</section>
