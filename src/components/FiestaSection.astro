---
interface Props {
  // Encabezado principal
  title?: string; // "Fiesta"
  intro?: string; // "Hagamos juntos una fiesta épica..."

  // Card 1 — Dress code
  dressTitle?: string; // "Dress code"
  dressDesc?: string; // "Una orientación para tu vestuario."
  dressHref?: string; // enlace a sección/modal/otra página

  // Card 2 — Restricciones
  rulesTitle?: string; // "Restricciones"
  rulesDesc?: string; // "Información adicional que se debe cumplir."
  rulesHref?: string;

  // Lluvia de sobres
  giftsTitle?: string; // "Lluvia de sobres"
  giftsDesc?: string;
  giftsHref?: string; // enlace a opciones (transferencias, etc.)

  class?: string; // clases extra del contenedor
}

// Valores por defecto estilo maqueta
const {
  title = "Fiesta",
  intro = "Hagamos juntos una fiesta épica. Aquí algunos detalles a tener en cuenta.",
  dressTitle = "Dress code",
  dressDesc = "Una orientación para tu vestuario.",
  dressHref = "#dresscode",
  rulesTitle = "Restricciones",
  rulesDesc = "Información adicional que se debe cumplir.",
  rulesHref = "#restricciones",
  giftsTitle = "Lluvia de sobres",
  giftsDesc = "Preferimos que lo presentes que deseen hacernos sean en efectivo o si eres como Pablo y no te gusta cargar efectivo aqui te dejamos otras opciones:",
  giftsHref = "#regalos",
  class: extra = "",
} = Astro.props;
---

<section
  class={`mx-auto w-full max-w-[900px] px-4 py-10 ${extra}`}
  style="--cream:#FBF4E8; --sepia:#8B6B5E; --accent:#D8C5A8;"
>
  <!-- Título con líneas -->
  <header class="text-center text-[color:var(--sepia)]">
    <div class="flex justify-center">
      <div class="flex justify-center border-y border-[#B88358] w-48">
        <h2
          id="guests-title"
          class="relative inline-block font-italianno text-6xl md:text-4xl text-[#B88358]"
        >
          <span
            class="before:absolute before:-top-3 before:left-1/2 before:-translate-x-1/2 before:block before:h-px before:w-40 before:bg-[color:var(--accent)/.7]
                 after:absolute after:-bottom-3 after:left-1/2 after:-translate-x-1/2 after:block after:h-px after:w-40 after:bg-[color:var(--accent)/.7]"
          >
            {title}
          </span>
        </h2>
      </div>
    </div>

    <p class="mx-auto mt-4 max-w-[36ch] font-italianno text-[#B88358] text-3xl">
      {intro}
    </p>
  </header>

  <!-- Tarjetas: mobile en columna / desktop apiladas con buen aire -->
  <div class="mt-10 grid gap-8 md:grid-cols-2 md:gap-10">
    <!-- Card Dress code -->
    <article class="rounded-[24px] p-6 text-center text-[#B88358] shadow-2xl">
      <h3 class="font-italianno text-5xl md:text-5xl mb-2">{dressTitle}</h3>

      <!-- Icono (saco + vestido) -->
      <div class="flex justify-center mx-auto">
        <svg
          width="95"
          height="100"
          viewBox="0 0 75 50"
          fill="none"
          xmlns="http://www.w3.org/2000/svg"
          xmlns:xlink="http://www.w3.org/1999/xlink"
        >
          <rect
            width="48"
            height="48"
            transform="matrix(-1 0 0 1 75 2)"
            fill="url(#pattern0_0_1)"></rect>
          <rect width="49" height="49" fill="url(#pattern1_0_1)"></rect>
          <defs>
            <pattern
              id="pattern0_0_1"
              patternContentUnits="objectBoundingBox"
              width="1"
              height="1"
            >
              <use xlink:href="#image0_0_1" transform="scale(0.00195312)"></use>
            </pattern>
            <pattern
              id="pattern1_0_1"
              patternContentUnits="objectBoundingBox"
              width="1"
              height="1"
            >
              <use xlink:href="#image1_0_1" transform="scale(0.00195312)"></use>
            </pattern>
            <image
              id="image0_0_1"
              width="512"
              height="512"
              preserveAspectRatio="none"
              xlink:href="data:image/png;base64,"
            />
            <image
              id="image1_0_1"
              width="512"
              height="512"
              preserveAspectRatio="none"
              xlink:href="data:image/png;base64,"
            />
          </defs>
        </svg>
      </div>

      <p class="font-italianno opacity-80 text-2xl text-[#B88358]">
        {dressDesc}
      </p>
      <a
        href={dressHref}
        class="h-10 py-2 w-48 mt-5 inline-block font-medium rounded-2xl bg-[#E6DDC0] text-[#B89477] hover:bg-[color:var(--sepia)]/15 transition"
        >Ver más</a
      >
    </article>

    <!-- Card Restricciones -->
    <article
      class="rounded-[24px] p-6 text-center text-[#B88358] shadow-2xl mt-16"
    >
      <h3 class="font-italianno text-5xl md:text-5xl mb-2">{rulesTitle}</h3>

      <!-- Icono (mano / aviso) -->
      <div class="flex justify-center mx-auto">
        <svg
          width="80"
          height="90"
          viewBox="0 0 55 55"
          fill="none"
          xmlns="http://www.w3.org/2000/svg"
          xmlns:xlink="http://www.w3.org/1999/xlink"
        >
          <rect width="55" height="55" fill="url(#pattern0_22_104)"></rect>
          <defs>
            <pattern
              id="pattern0_22_104"
              patternContentUnits="objectBoundingBox"
              width="1"
              height="1"
            >
              <use xlink:href="#image0_22_104" transform="scale(0.00195312)"
              ></use>
            </pattern>
            <image
              id="image0_22_104"
              width="512"
              height="512"
              preserveAspectRatio="none"
              xlink:href="data:image/png;base64,"
            />
          </defs>
        </svg>
      </div>

      <p class="font-italianno opacity-80 text-2xl text-[#B88358]">
        {rulesDesc}
      </p>
      <a
        href={rulesHref}
        class="h-10 py-2 w-48 mt-5 inline-block font-medium rounded-2xl bg-[#E6DDC0] text-[#B89477] hover:bg-[color:var(--sepia)]/15 transition"
        >Ver más</a
      >
    </article>
  </div>

  <!-- Lluvia de sobres -->
  <section class="mt-12 text-center text-[color:var(--sepia)]">
    <div class="flex justify-center">
      <div class="flex justify-center border-y border-[#B88358] w-72">
        <h2
          id="guests-title"
          class="relative inline-block font-italianno text-6xl md:text-4xl text-[#B88358]"
        >
          <span
            class="before:absolute before:-top-3 before:left-1/2 before:-translate-x-1/2 before:block before:h-px before:w-40 before:bg-[color:var(--accent)/.7]
                 after:absolute after:-bottom-3 after:left-1/2 after:-translate-x-1/2 after:block after:h-px after:w-40 after:bg-[color:var(--accent)/.7]"
          >
            {giftsTitle}
          </span>
        </h2>
      </div>
    </div>

    <p class="font-italianno opacity-80 text-3xl text-[#B88358] pt-10">
      {giftsDesc}
    </p>

    <a
      href={giftsHref}
      class="h-10 py-2 w-48 mt-5 inline-block font-medium rounded-2xl bg-[#E6DDC0] text-[#B89477] hover:bg-[color:var(--sepia)]/15 transition"
      >Ver opciones</a
    >
  </section>
</section>
